/*主导航菜单css*/
#panel_nav{
    float:left;
    width: 160px;
    height: 23px;
}
#panel_nav a.handler{
    width: 110px;
    padding-left: 40px;
    color: #fff;
    font-weight:bold;
}
#panel_nav a:hover.handler{
    background: none;
    color: #fff;
}
#panel_nav, #panel_nav ul{
    margin:0;
    padding:0;
    list-style-type:none;
    list-style-position:outside;
    position:relative;
}

#panel_nav a{
    display:block;
    padding:0px 5px;
    color:#E57D37;
    text-decoration:none;
    font-weight: normal;
}

#panel_nav a:hover{
    color:#E57D37;
}

#panel_nav li{
    float:left;
    position:relative;
}

#panel_nav ul {
    position:absolute;
    display:none;
    top:25px;
}

#panel_nav li ul a{
    float:left;
}

#panel_nav ul ul{
    top:auto;
}

#panel_nav li ul ul {
    margin:0px 0 0 10px;
}

#panel_nav li:hover ul ul, #panel_nav li:hover ul ul ul, #panel_nav li:hover ul ul ul ul{
    display:none;
}
#panel_nav li:hover ul, #panel_nav li li:hover ul, #panel_nav li li li:hover ul, #panel_nav li li li li:hover ul{
    display:block;
}
#panel_nav ul.inlist{
    display: block;
    /*visibility: hidden;*/
    background: url(/images/le_25.jpg) no-repeat left top;
    height: 165px;
}
#panel_nav ul.inlist a{
    padding-left: 45px;
    width:113px;
    height:26px;
    color:#333;
    line-height: 26px;
}
#panel_nav ul.inlist a:hover{
    height:26px;
    line-height: 26px;
    color:#CC0000;
    text-decoration: underline;
}
#panel_nav li ul.inpanel{
    width: 180px;
    display: block;
    visibility: hidden;
    /*    border: 1px solid #F5B666;*/
    left:130px;
    background:#fef9f6 ;
    border: 1px solid #ccc;
    border-left: none;

}
#panel_nav li .inpanel li{
    float:left;

}
#panel_nav li .inpanel li a{
    padding-left: 5px;
    width: 60px;
    height:25px;
}
#panel_nav li .inpanel li a:hover{
    background: #eee;
    border:none;
}
